{
  "extends": ["next", "plugin:prettier/recommended"],
  "plugins": ["testing-library"],
  "overrides": [
    {
      "files": [
        "**/?(*.)+(spec|test).[jt]s?(x)"
      ],
      "extends": ["plugin:testing-library/react"]
    }
  ],
  "rules": {
    "no-unused-vars": "error",
    "import/no-anonymous-default-export": "off"
  }
}
